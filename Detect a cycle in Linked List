// Striver;s SDE Sheet Challenge Day-6
bool detectCycle(Node *head)
{
    //    Write your code here
    
    Node *fast=head;
    Node *slow=head;
    
    while(fast->next!=NULL && fast->next->next!=NULL){
        slow=slow->next;
        fast=fast->next->next;
        if(slow==fast){
            return true;
        }
    }
    return false;
    
}
