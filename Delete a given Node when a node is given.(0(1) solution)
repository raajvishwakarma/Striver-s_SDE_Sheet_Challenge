// Striver's SDE Sheet Challenge Day - 5
void deleteNode(LinkedListNode<int> * node) {
    LinkedListNode<int>* temp =node->next;
    //store the data of temp as node->data
    //and make node->next=temp->next;
    node->data =temp->data;
    node->next =temp->next;
    
    //delete temp from memory
    temp->next=NULL;
    delete temp;
    
}
